{
  "rules": {
    "import/no-unresolved": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "warn"
  },
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier",
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  }
}
